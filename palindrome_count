#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int T,i,n,j,c,p=0;
    cin>>T;
    
    for(i=0;i<T;i++)
    {
        cin>>n;
        c = n/2;
        cout<<"1"<<" "<<"1";
        for(j=0;j<c;j++)
        {
            cout<<p;
        }
        cout<<"\n";
    }
}
